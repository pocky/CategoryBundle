{% extends 'BlackCategoryBundle:AdminCategory:base.html.twig' %}

{% block category_content %}
<form id="validate" class="main grid9" action="{{ path('admin_category_batch') }}" method="post">
    <div class="fluid">
        <div class="widget check">
            <div class="whead">
                <span class="titleIcon"><input type="checkbox" id="titleCheck" name="titleCheck" /></span>
                <h6>{% trans from "views" %}category.admin.index.category.text{% endtrans %}</h6>
                <div class="clear"></div>
            </div>

            {% include 'BlackCategoryBundle:AdminCategory:table.html.twig' with {'keys': keys, 'csrf': csrf} %}
        </div>
    </div>
</form>
{% endblock %}
